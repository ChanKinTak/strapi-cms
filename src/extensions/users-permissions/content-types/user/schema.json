{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "employeeId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "isFirstLogin": {
      "type": "boolean",
      "default": true
    },
    "participation_response": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::participation-response.participation-response",
      "mappedBy": "user"
    },
    "registration_settings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::registration-setting.registration-setting",
      "mappedBy": "authorizedEditors"
    },
    "department": {
      "type": "enumeration",
      "enum": [
        "department01",
        "department02"
      ]
    },
    "team": {
      "type": "enumeration",
      "enum": [
        "team01",
        "team02"
      ]
    },
    "Group": {
      "type": "enumeration",
      "enum": [
        "Group01",
        "Group02",
        "Group03"
      ]
    },
    "registration": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::registration.registration",
      "mappedBy": "relation"
    },
    "sentRoommateRequests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::roommate-request.roommate-request",
      "mappedBy": "requesting_user"
    },
    "confitmRoommateRequest": {
      "type": "boolean"
    },
    "full_name": {
      "type": "string"
    },
    "receivedRoommateRequests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::roommate-request.roommate-request",
      "mappedBy": "receive_user"
    },
    "notification_reads_user": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification-read.notification-read",
      "mappedBy": "user"
    },
    "notification": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::notification.notification",
      "inversedBy": "users"
    }
  }
}
