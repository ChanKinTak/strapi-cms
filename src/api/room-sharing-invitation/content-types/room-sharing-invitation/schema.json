{
  "kind": "collectionType",
  "collectionName": "room_sharing_invitations",
  "info": {
    "singularName": "room-sharing-invitation",
    "pluralName": "room-sharing-invitations",
    "displayName": "Room Sharing Invitation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sender": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "sender_user"
    },
    "receiver": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "receiver_user"
    },
    "registration": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::conference-registration.conference-registration",
      "inversedBy": "registration_reg"
    },
    "room_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "accepted",
        "declined"
      ],
      "default": "pending"
    },
    "notification_relatedInvitation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::notification.notification",
      "mappedBy": "related_invitation"
    }
  }
}
