{
  "kind": "collectionType",
  "collectionName": "roommate_requests",
  "info": {
    "singularName": "roommate-request",
    "pluralName": "roommate-requests",
    "displayName": "Roommate requests"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
