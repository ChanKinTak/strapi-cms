{
  "kind": "collectionType",
  "collectionName": "notifications",
  "info": {
    "singularName": "notification",
    "pluralName": "notifications",
    "displayName": "Notifications"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "info",
        "warning",
        "error",
        "success"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "notifications"
    },
    "read": {
      "type": "boolean"
    },
    "pushSent": {
      "type": "boolean",
      "default": false
    },
    "deviceTokens": {
      "type": "json"
    }
  }
}
